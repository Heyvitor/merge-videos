{
    "build": {
      "builder": "nixpacks",
      "buildCommand": "pip install -r requirements.txt",
      "watchPatterns": [
        "**/*.py",
        "requirements.txt",
        "web/**/*"
      ]
    },
    "deploy": {
      "startCommand": "python main.py",
      "restartPolicy": "on-failure",
      "restartPolicyMaxRetries": 10
    },
    "healthcheck": {
      "path": "/",
      "intervalSeconds": 30,
      "timeoutSeconds": 10,
      "retries": 3
    }
  }